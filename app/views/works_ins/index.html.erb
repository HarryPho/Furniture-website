<h1>Listing works_ins</h1>

<table>
  <tr>
    <th>Employee</th>
    <th>Work center</th>
  </tr>

<% @works_ins.each do |works_in| %>
  <tr>
    <td><%=h works_in.employee_id %></td>
    <td><%=h works_in.work_center_id %></td>
    <td><%= link_to 'Show', works_in %></td>
    <td><%= link_to 'Edit', edit_works_in_path(works_in) %></td>
    <td><%= link_to 'Destroy', works_in, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New works_in', new_works_in_path %>