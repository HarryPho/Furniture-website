<h1>Listing customers</h1>

<table>
  <tr>
    <th>Customer</th>
    <th>Name</th>
    <th>Address</th>
    <th>City</th>
    <th>C state</th>
    <th>Zipcode</th>
    <th>Email</th>
    <th>Username</th>
    <th>Password</th>
    <th>C type</th>
  </tr>

<% @customers.each do |customer| %>
  <tr>
    <td><%=h customer.customer_id %></td>
    <td><%=h customer.name %></td>
    <td><%=h customer.address %></td>
    <td><%=h customer.city %></td>
    <td><%=h customer.c_state %></td>
    <td><%=h customer.zipcode %></td>
    <td><%=h customer.email %></td>
    <td><%=h customer.username %></td>
    <td><%=h customer.password %></td>
    <td><%=h customer.c_type %></td>
    <td><%= link_to 'Show', customer %></td>
    <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
    <td><%= link_to 'Destroy', customer, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New customer', new_customer_path %>