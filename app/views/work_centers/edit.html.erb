<h1>Editing work_center</h1>

<% form_for(@work_center) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :work_center_id %><br />
    <%= f.text_field :work_center_id %>
  </p>
  <p>
    <%= f.label :location %><br />
    <%= f.text_field :location %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @work_center %> |
<%= link_to 'Back', work_centers_path %>