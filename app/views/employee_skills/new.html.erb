<h1>New employee_skill</h1>

<% form_for(@employee_skill) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :employee_id %><br />
    <%= f.text_field :employee_id %>
  </p>
  <p>
    <%= f.label :skill_id %><br />
    <%= f.text_field :skill_id %>
  </p>
  <p>
    <%= f.label :qualify_date %><br />
    <%= f.date_select :qualify_date %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', employee_skills_path %>