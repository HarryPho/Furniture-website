<h1>Listing employee_skills</h1>

<table>
  <tr>
    <th>Employee</th>
    <th>Skill</th>
    <th>Qualify date</th>
  </tr>

<% @employee_skills.each do |employee_skill| %>
  <tr>
    <td><%=h employee_skill.employee_id %></td>
    <td><%=h employee_skill.skill_id %></td>
    <td><%=h employee_skill.qualify_date %></td>
    <td><%= link_to 'Show', employee_skill %></td>
    <td><%= link_to 'Edit', edit_employee_skill_path(employee_skill) %></td>
    <td><%= link_to 'Destroy', employee_skill, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New employee_skill', new_employee_skill_path %>