<h1>Listing product_lines</h1>

<table>
  <tr>
    <th>Product line</th>
    <th>Name</th>
    <th>Annual sales goal</th>
  </tr>

<% @product_lines.each do |product_line| %>
  <tr>
    <td><%=h product_line.product_line_id %></td>
    <td><%=h product_line.name %></td>
    <td><%=h product_line.annual_sales_goal %></td>
    <td><%= link_to 'Show', product_line %></td>
    <td><%= link_to 'Edit', edit_product_line_path(product_line) %></td>
    <td><%= link_to 'Destroy', product_line, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_line', new_product_line_path %>