<h1>Editing testimonial</h1>

<% form_for(@testimonial) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :testimonial_id %><br />
    <%= f.text_field :testimonial_id %>
  </p>
  <p>
    <%= f.label :testimonial_content %><br />
    <%= f.text_area :testimonial_content %>
  </p>
  <p>
    <%= f.label :testimonial_date %><br />
    <%= f.date_select :testimonial_date %>
  </p>
  <p>
    <%= f.label :customer_id %><br />
    <%= f.text_field :customer_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @testimonial %> |
<%= link_to 'Back', testimonials_path %>