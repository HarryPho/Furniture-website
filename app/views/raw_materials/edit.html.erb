<h1>Editing raw_material</h1>

<% form_for(@raw_material) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :raw_material_id %><br />
    <%= f.text_field :raw_material_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :unit_of_measure %><br />
    <%= f.text_field :unit_of_measure %>
  </p>
  <p>
    <%= f.label :standard_cost %><br />
    <%= f.text_field :standard_cost %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @raw_material %> |
<%= link_to 'Back', raw_materials_path %>