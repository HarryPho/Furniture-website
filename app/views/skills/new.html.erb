<h1>New skill</h1>

<% form_for(@skill) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :skill_id %><br />
    <%= f.text_field :skill_id %>
  </p>
  <p>
    <%= f.label :skill_description %><br />
    <%= f.text_field :skill_description %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', skills_path %>