<h1>New territory</h1>

<% form_for(@territory) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :territory_id %><br />
    <%= f.text_field :territory_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', territories_path %>